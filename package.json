{
  "name": "sanity-plugin-soundcloud-input",
  "version": "1.0.0",
  "description": "A plugin for using SoundCloud data in Sanity.",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/damianrosellen1/sanity-plugin-soundcloud-input#readme",
  "bugs": {
    "url": "https://github.com/damianrosellen1/sanity-plugin-soundcloud-input/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/damianrosellen1/sanity-plugin-soundcloud-input.git"
  },
  "license": "MIT",
  "author": "Damian Rosellen <hi@damianrosellen.de>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "dependencies": {
    "@sanity/browserslist-config": "^1.0.3",
    "@sanity/icons": "^3.4.0",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^2.8.23",
    "browserslist": "extends @sanity/browserslist-config",
    "yalc": "1.0.0-pre.53"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "^6.11.10",
    "@sanity/plugin-kit": "^4.0.18",
    "@types/react": "^18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.64.0",
    "styled-components": "^6.1.13",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=18"
  }
}
