{
  "extends": "./tsconfig.settings",
  "include": ["./src", "./package.config.ts"],
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "jsx": "react"
  }
}
